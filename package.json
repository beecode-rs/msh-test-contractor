{
	"name": "@beecode/msh-test-contractor",
	"version": "0.3.9",
	"description": "Test Contractor",
	"keywords": [],
	"homepage": "https://github.com/beecode-rs/msh-test-contractor#readme",
	"bugs": {
		"url": "https://github.com/beecode-rs/msh-test-contractor/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/beecode-rs/msh-test-contractor.git"
	},
	"license": "mit",
	"author": "Milos Bugarinovic <milos.bugarinovic@gmail.com> (https://milos.bugarinovic.com)",
	"type": "module",
	"imports": {
		"#/*": "./dist/esm/*.js"
	},
	"exports": {
		".": {
			"import": {
				"default": "./dist/esm/index.js",
				"types": "./dist/types-esm/index.d.ts"
			},
			"require": {
				"default": "./dist/cjs/index.js",
				"types": "./dist/types-cjs/index.d.ts"
			}
		},
		"./*": {
			"import": {
				"default": "./dist/esm/*.js",
				"types": "./dist/types-esm/*.d.ts"
			},
			"require": {
				"default": "./dist/cjs/*.js",
				"types": "./dist/types-cjs/*.d.ts"
			}
		}
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/types-ems/index.d.ts",
	"files": [
		"./dist"
	],
	"scripts": {
		"build": "npm run clean && npm run npm:esm:package:copy && npm run tsc",
		"build:cjs": "npm run clean:cjs && npm run tsc:cjs && npm run tsc:cjs:alias",
		"build:on-success": "npm run tsc:cleaner",
		"build:watch": "npm run clean && npm run npm:esm:package:copy && npm run tsc:watch -- --onCompilationComplete 'npm run build:on-success'",
		"bump-version": "npm --no-git-tag-version version",
		"clean": "rimraf ./dist/esm ./dist/types-esm",
		"clean:cjs": "rimraf ./dist/cjs ./dist/types-cjs",
		"doc:api": "npx typedoc",
		"doc:toc": "npx markdown-toc -i ./README.md",
		"docker:build": "docker build -f ./resource/docker/Dockerfile -t bc-msh-test-contract ./",
		"docker:exec": "docker run --rm bc-msh-test-contract",
		"docker:exec:sh": "docker run --rm -it bc-msh-test-contract sh",
		"docker:rmi": "docker rmi bc-msh-test-contract",
		"docker:run:package-lock-update": "docker run --rm -v ./:/usr/app/ milosbugarinovic/msh-lib-base:node18.15.0-alpine3.17 npm i --package-lock-only",
		"eslint": "eslint ./ --quiet",
		"eslint:fix": "npm run eslint -- --fix",
		"initial-setup": "npm run initial-setup:husky && npm run initial-setup:git-config",
		"initial-setup:git-config": "git config include.path ./.git-config",
		"initial-setup:husky": "npx husky install",
		"jest": "NODE_OPTIONS=--experimental-vm-modules npx jest",
		"jsonlint": "npm run jsonlint:fix -- -c",
		"jsonlint:fix": "jsonsort * .*.json '!./test/node_modules' '!./resource' '!**/.env*' -t",
		"lint": "npm run prettier && npm run eslint && npm run jsonlint",
		"lint:fix": "npm run prettier:fix && npm run eslint:fix && npm run jsonlint:fix",
		"npm:esm:package:copy": "mkdir -p ./dist/esm && cp ./resource/tsc/package.json ./dist/esm/",
		"pack": "npm pack --pack-destination=./packages",
		"prettier": "prettier --check {**/*,*}.{js,jsx,ts,tsx}",
		"prettier:fix": "prettier --write {**/*,*}.{js,jsx,ts,tsx}",
		"semantic-release": "semantic-release",
		"semantic-release:check": "npm run semantic-release -- --dry-run --no-ci",
		"test": "npm run test:unit #&& npm run test:int",
		"test:e2e": "TZ=utc npm run jest -- --config=./test/jest.config.js",
		"test:int": "TZ=utc npm run jest -- --config=./jest.config.int.js",
		"test:unit": "TZ=utc npm run jest -- --config=./jest.config.unit.js",
		"test:unit:coverage": "npm run test:unit -- --coverage",
		"test:unit:coverage:publish": "codecov",
		"tsc": "tsc -p ./tsconfig.esm.json",
		"tsc:check": "npm run tsc -- --noEmit",
		"tsc:check:list-files": "npm run tsc:check -- --listFiles",
		"tsc:cjs": "tsc -p ./tsconfig.cjs.json",
		"tsc:cjs:alias": "tsc-alias -p ./tsconfig.cjs.json",
		"tsc:cleaner": "ts-cleaner --dist ./dist/esm",
		"tsc:cleaner:watch": "npm run tsc:cleaner -- --watch",
		"tsc:time": "npm run tsc -- --diagnostics",
		"tsc:watch": "tsc-watch -p ./tsconfig.esm.json --preserveWatchOutput"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"**.*.json": [
			"npm run jsonlint"
		],
		"**/*.{ts,tsx,js,jsx}": [
			"eslint",
			"prettier --check"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@beecode/msh-logger": "0.2.1",
		"deep-equal": "2.2.2",
		"dotenv": "16.3.1",
		"glob": "10.3.3"
	},
	"devDependencies": {
		"@commitlint/cli": "17.6.7",
		"@commitlint/config-conventional": "17.6.7",
		"@commitlint/prompt": "17.6.7",
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/commit-analyzer": "10.0.1",
		"@semantic-release/exec": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "9.0.4",
		"@semantic-release/release-notes-generator": "11.0.4",
		"@types/deep-equal": "1.0.1",
		"@types/dotenv": "8.2.0",
		"@types/glob": "8.1.0",
		"@types/jest": "29.5.3",
		"@types/node": "20.4.2",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"commitizen": "4.3.0",
		"eslint": "8.45.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-jest": "27.2.3",
		"eslint-plugin-no-loops": "0.3.0",
		"eslint-plugin-no-only-tests": "3.1.0",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-sort-keys-fix": "1.1.2",
		"husky": "8.0.3",
		"jest": "29.6.1",
		"jest-extended": "4.0.0",
		"json-sort-cli": "3.1.4",
		"lint-staged": "13.2.3",
		"markdown-toc": "1.2.0",
		"prettier": "3.0.0",
		"rimraf": "5.0.1",
		"semantic-release": "21.0.7",
		"source-map-support": "0.5.21",
		"ts-cleaner": "1.0.5",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.1",
		"tsc-alias": "1.8.7",
		"tsc-watch": "6.0.4",
		"typedoc": "0.24.8",
		"typedoc-plugin-markdown": "3.15.3",
		"typescript": "5.1.6"
	},
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	}
}
