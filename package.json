{
	"name": "@beecode/msh-test-contractor",
	"version": "0.3.9",
	"description": "Test Contractor",
	"keywords": [],
	"homepage": "https://github.com/beecode-rs/msh-test-contractor#readme",
	"bugs": {
		"url": "https://github.com/beecode-rs/msh-test-contractor/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/beecode-rs/msh-test-contractor.git"
	},
	"license": "mit",
	"type": "module",
	"author": "Milos Bugarinovic <milos.bugarinovic@gmail.com> (https://milos.bugarinovic.com)",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./*": {
			"import": "./dist/esm/*.js",
			"require": "./dist/cjs/*.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"files": [
		"./dist/cjs",
		"./dist/esm"
	],
	"scripts": {
		"alias": "concurrently -c auto 'npm:alias:*'",
		"alias:cjs": "tsc-alias -p ./tsconfig.cjs.alias-paths.json",
		"alias:esm": "tsc-alias -p ./tsconfig.esm.alias-paths.json",
		"build": "concurrently -c auto 'npm:build:*'",
		"build:cjs": "npm run clean:cjs && npm run tsc:cjs && npm run alias:cjs && npm run tsc-hybrid-lib-fix",
		"build:esm": "npm run clean:esm && npm run tsc:esm && npm run alias:esm && npm run tsc-hybrid-lib-fix",
		"clean": "concurrently -c auto 'npm:clean:*'",
		"clean:cjs": "rimraf ./dist/cjs",
		"clean:esm": "rimraf ./dist/esm",
		"doc": "concurrently -c auto 'npm:doc:*'",
		"doc:api": "npx typedoc",
		"doc:toc": "npx markdown-toc -i ./README.md",
		"docker-build": "docker build -f ./resource/docker/Dockerfile -t bc-msh-test-contractor ./",
		"docker-rmi": "docker rmi bc-msh-test-contractor",
		"docker-run-package-lock-update": "docker run --rm -v ./:/usr/app/ milosbugarinovic/msh-lib-base:node18.17.0-alpine3.18 npm i --package-lock-only",
		"docker-run-sh": "docker run --rm -it bc-msh-test-contractor sh",
		"init": "concurrently -c auto 'npm:init:*'",
		"init:git-config": "git config include.path ./.git-config",
		"init:husky": "npx husky install",
		"jest": "npx jest",
		"lint": "npm run lint-prettier && npm run lint-eslint && npm run lint-json",
		"lint-eslint": "eslint ./ --quiet",
		"lint-eslint-fix": "npm run lint-eslint -- --fix",
		"lint-fix": "npm run lint-prettier-fix && npm run lint-eslint-fix && npm run lint-json-fix",
		"lint-json": "npm run lint-json-fix -- -c",
		"lint-json-fix": "jsonsort * .*.json '!./test/node_modules' '!./resource' '!**/.env*' -t",
		"lint-prettier": "prettier --check {**/*,*}.{js,jsx,ts,tsx}",
		"lint-prettier-fix": "prettier --write {**/*,*}.{js,jsx,ts,tsx}",
		"npm-bump-version": "npm --no-git-tag-version version",
		"npm-pack": "mkdir -p packages && npm pack --pack-destination=./packages",
		"npm-semantic-release": "semantic-release",
		"npm-semantic-release-check": "npm run npm-semantic-release -- --dry-run --no-ci",
		"test": "concurrently -c auto 'npm:test:*'",
		"test-coverage-unit": "npm run test:unit -- --coverage",
		"test-coverage-unit-publish": "codecov",
		"test:e2e": "TZ=utc npm run jest -- --config=./test/jest.config.mjs",
		"test:int": "TZ=utc npm run jest -- --config=./jest.config.int.ts",
		"test:unit": "TZ=utc npm run jest -- --config=./jest.config.unit.ts",
		"tsc": "concurrently -c auto 'npm:tsc:*'",
		"tsc-check": "npm run tsc:esm -- --noEmit",
		"tsc-cleaner": "concurrently -c auto 'npm:tsc-cleaner:*'",
		"tsc-cleaner-watch": "concurrently -c auto 'npm:tsc-cleaner-watch:*'",
		"tsc-cleaner-watch:cjs": "npm run tsc-cleaner:cjs -- --watch",
		"tsc-cleaner-watch:esm": "npm run tsc-cleaner:esm -- --watch",
		"tsc-cleaner:cjs": "ts-cleaner --dist ./dist/cjs",
		"tsc-cleaner:esm": "ts-cleaner --dist ./dist/esm",
		"tsc-hybrid-lib-fix": "./resource/script/tsc/hybrid-lib-fix.sh",
		"tsc-time": "npm run tsc:esm -- --diagnostics",
		"tsc-watch": "concurrently -c auto 'npm:tsc-watch:*'",
		"tsc-watch:cjs": "npm run tsc:cjs -- -w --preserveWatchOutput",
		"tsc-watch:esm": "npm run tsc:esm -- -w --preserveWatchOutput",
		"tsc:cjs": "tsc -p ./tsconfig.cjs.json",
		"tsc:esm": "tsc -p ./tsconfig.esm.json",
		"watch": "concurrently -c auto 'npm:watch:*'",
		"watch:cjs": "npm run tsc-hybrid-lib-fix && concurrently -c auto -n build,clean,alias 'npm run tsc-watch:cjs' 'npm run tsc-cleaner-watch:cjs' 'npm run alias:cjs'",
		"watch:esm": "npm run tsc-hybrid-lib-fix && concurrently -c auto -n build,clean,alias 'npm run tsc-watch:esm' 'npm run tsc-cleaner-watch:esm' 'npm run alias:esm'"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"**/*.json": [
			"npm run lint-json"
		],
		"**/*.{ts,tsx,js,jsx}": [
			"eslint",
			"prettier --check"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@beecode/msh-logger": "0.2.1",
		"dotenv": "16.4.5",
		"fast-deep-equal": "3.1.3",
		"glob": "10.3.10"
	},
	"devDependencies": {
		"@commitlint/cli": "19.1.0",
		"@commitlint/config-conventional": "19.1.0",
		"@commitlint/prompt": "19.1.0",
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/commit-analyzer": "11.1.0",
		"@semantic-release/exec": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "9.2.6",
		"@semantic-release/release-notes-generator": "12.1.0",
		"@types/deep-equal": "1.0.4",
		"@types/glob": "8.1.0",
		"@types/node": "20.11.26",
		"@typescript-eslint/eslint-plugin": "7.2.0",
		"@typescript-eslint/parser": "7.2.0",
		"commitizen": "4.3.0",
		"concurrently": "8.2.2",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jest": "27.9.0",
		"eslint-plugin-no-loops": "0.3.0",
		"eslint-plugin-no-only-tests": "3.1.0",
		"eslint-plugin-prettier": "5.1.3",
		"eslint-plugin-sort-keys-fix": "1.1.2",
		"husky": "9.0.11",
		"jest": "29.7.0",
		"jest-extended": "4.0.2",
		"json-sort-cli": "4.0.2",
		"lint-staged": "15.2.2",
		"markdown-toc": "1.2.0",
		"prettier": "3.2.5",
		"rimraf": "5.0.5",
		"semantic-release": "22.0.12",
		"source-map-support": "0.5.21",
		"ts-cleaner": "1.0.5",
		"ts-jest": "29.1.2",
		"ts-node": "10.9.2",
		"tsc-alias": "1.8.8",
		"tsc-watch": "6.0.4",
		"typedoc": "0.25.12",
		"typedoc-plugin-markdown": "3.17.1",
		"typescript": "5.3.3"
	},
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	}
}
